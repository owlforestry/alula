.smilies-worried,
.smilies-wink,
.smilies-uncertain,
.smilies-surprise,
.smilies-stock_right,
.smilies-smirk,
.smilies-smile,
.smilies-smile-big,
.smilies-sad,
.smilies-raspberry,
.smilies-question,
.smilies-plain,
.smilies-monkey,
.smilies-laugh,
.smilies-kiss,
.smilies-idea,
.smilies-exclaim,
.smilies-embarrassed,
.smilies-devilish,
.smilies-crying,
.smilies-cool,
.smilies-angry,
.smilies-angel{background-image:url(<%= asset_url("smilies.png")%>);background-repeat:no-repeat;display:inline-block;text-indent:-9999px}
.smilies-worried{background-position:0px 0px;width:16px;height:16px;}
.smilies-wink{background-position:-16px 0px;width:16px;height:16px;}
.smilies-uncertain{background-position:0px -16px;width:16px;height:16px;}
.smilies-surprise{background-position:-16px -16px;width:16px;height:16px;}
.smilies-stock_right{background-position:-32px 0px;width:16px;height:16px;}
.smilies-smirk{background-position:-32px -16px;width:16px;height:16px;}
.smilies-smile{background-position:0px -32px;width:16px;height:16px;}
.smilies-smile-big{background-position:-16px -32px;width:16px;height:16px;}
.smilies-sad{background-position:-32px -32px;width:16px;height:16px;}
.smilies-raspberry{background-position:-48px 0px;width:16px;height:16px;}
.smilies-question{background-position:-48px -16px;width:16px;height:16px;}
.smilies-plain{background-position:-48px -32px;width:16px;height:16px;}
.smilies-monkey{background-position:0px -48px;width:16px;height:16px;}
.smilies-laugh{background-position:-16px -48px;width:16px;height:16px;}
.smilies-kiss{background-position:-32px -48px;width:16px;height:16px;}
.smilies-idea{background-position:-48px -48px;width:16px;height:16px;}
.smilies-exclaim{background-position:-64px 0px;width:16px;height:16px;}
.smilies-embarrassed{background-position:-64px -16px;width:16px;height:16px;}
.smilies-devilish{background-position:-64px -32px;width:16px;height:16px;}
.smilies-crying{background-position:-64px -48px;width:16px;height:16px;}
.smilies-cool{background-position:0px -64px;width:16px;height:16px;}
.smilies-angry{background-position:-16px -64px;width:16px;height:16px;}
.smilies-angel{background-position:-32px -64px;width:16px;height:16px;}
